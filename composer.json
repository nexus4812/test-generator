{
    "name": "nexus4812/test-generator",
    "require": {
        "openai-php/client": "^0.10.1",
        "symfony/http-client": "^7.1",
        "nyholm/psr7": "^1.8",
        "phpunit/phpunit": "^11.3",
        "symfony/console": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Nexus4812\\TestGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexus4812\\TestGenerator\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "bin": [
        "nexus-test-generator.php"
    ],
    "require-dev": {
        "mockery/mockery": "^1.6"
    }
}
